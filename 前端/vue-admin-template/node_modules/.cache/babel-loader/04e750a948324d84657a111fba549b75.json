   });\r\n  },\r\n\r\n  // 更新\r\n  updateById(teacher) {\r\n    return request({\r\n      url: `${api_name}/${teacher.id}`,\r\n      method: \"put\",\r\n      data: teacher\r\n    });\r\n  },\r\n\r\n  // 获取所有讲师\r\n  getList() {\r\n    return request({\r\n      url: api_name,\r\n      method: \"get\"\r\n    });\r\n  }\r\n};\r\n"]}]}